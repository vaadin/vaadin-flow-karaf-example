#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/cssesc@3.0.0/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/node_modules:/opt/agent/temp/buildTmp/node_modules:/opt/agent/temp/node_modules:/opt/agent/node_modules:/opt/node_modules:/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/postcss-selector-parser@6.0.4/node_modules/postcss-selector-parser/node_modules:/opt/agent/temp/buildTmp/base-starter-flow-karaf/starter/node_modules/.pnpm/postcss-selector-parser@6.0.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
fi
